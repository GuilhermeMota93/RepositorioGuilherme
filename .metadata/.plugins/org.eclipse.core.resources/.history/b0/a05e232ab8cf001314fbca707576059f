package com.example.itlog.requestobjects;

public class Projetos {
	String name, manager, descripiton, company;
	int projectid, hours;

	public Projetos() {

	}

	public Projetos(String nome, int id, String clienInfo, String gestor,
			String descricao, int horas) {
		this.name = nome;
		this.projectid = id;
		this.company = company;
		this.manager = gestor;
		this.descripiton = descricao;
		this.hours = horas;

	}

	public Projetos(String name, int projectid, String company, String manager,
			String description) {
		this.name = name;
		this.projectid = projectid;
		this.company = company;
		this.manager = manager;
		this.descripiton = description;
	}

	public String getNome() {
		return name;
	}

	public void setNome(String nome) {
		this.name = nome;
	}

	public String getGestor() {
		return manager;
	}

	public void setGestor(String gestor) {
		this.manager = gestor;
	}

	public String getDescricao() {
		return descripiton;
	}

	public void setDescricao(String descricao) {
		this.descripiton = descricao;
	}

	public String getClientInfo() {
		return company;
	}

	public void setClientInfo(String clientInfo) {
		this.company = clientInfo;
	}

	public int getId() {
		return projectid;
	}

	public void setId(int id) {
		this.projectid = id;
	}

	public int getHoras() {
		return hours;
	}

	public void setHoras(int horas) {
		this.hours = horas;
	}

}
