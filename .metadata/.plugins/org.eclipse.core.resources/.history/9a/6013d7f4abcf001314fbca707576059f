package com.example.itlog.requestobjects;

public class Projetos {
	String nome, gestor, descricao, clientInfo;
	int id, horas;

	public Projetos() {

	}

	public Projetos(String nome, int id, String gestor, String descricao,
			int horas) {
		this.nome = nome;
		this.gestor = gestor;
		this.descricao = descricao;
		this.id = id;
		this.horas = horas;

	}

	
}
