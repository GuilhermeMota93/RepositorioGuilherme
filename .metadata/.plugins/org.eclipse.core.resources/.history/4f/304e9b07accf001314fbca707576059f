package com.example.itlog.requestobjects;

public class Projetos {
	String nome, gestor, descricao, clientInfo;
	int id, horas;

	public Projetos() {

	}

	public Projetos(String nome, int id, String clienInfo, String gestor,
			String descricao, int horas) {
		this.nome = nome;
		this.id = id;
		this.clientInfo = clientInfo;
		this.gestor = gestor;
		this.descricao = descricao;
		this.horas = horas;

	}
	public Projetos(String nome, int id, String clienInfo, String gestor,
			String descricao){
		this.nome = nome;
		this.id = id;
		this.clientInfo = clientInfo;
		this.gestor = gestor;
		this.descricao = descricao;
	}

}
