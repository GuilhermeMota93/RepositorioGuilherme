package com.example.itlog;

public class Object_ERROR {
	String id="";
	String msg="";

	public Object_ERROR(String id, String msg) {
		super();
		this.id = id;
		this.msg = msg;
	}

	public Object_ERROR() {
		// TODO Auto-generated constructor stub
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getMsg() {
		return msg;
	}

	public void setMsg(String msg) {
		this.msg = msg;
	}

}
